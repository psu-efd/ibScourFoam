FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00018;

boundaryField
{
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    inlet
    {
        type            timeVaryingMappedFixedValue;
        mapMethod       nearest;
        offset          constant 0;
        value           uniform 0.00018;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            kqRWallFunction;
        value           uniform 0.00018;
    }
    munition
    {
        type            kqRWallFunction;
        value           uniform 0.00018;
    }
}


