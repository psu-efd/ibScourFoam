FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
libs
(
    "libIBMethod.so"
)

application     ibScourFoam;


startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         300;

deltaT          0.002;

writeControl    runTime;

writeInterval  	5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;



